<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle"
            >
            <!-- solid background -->
            <solid android:color="#0cd585"/>
            <!-- border-->
            <stroke
                android:color="#24ff20"
                android:width="2dp"
                />
            <corners android:radius="25dp"/>

        </shape>
    </item>
</selector>